<resource>
 <imageatlas file="tiles.png" size="32x32" >
  <image id="itl" offset="0,64" />
  <image id="ih" offset="32,64" />
  <image id="itr" offset="64,64" />
  <image id="iv" offset="0,32" />
  <image id="ie" offset="32,32" />
  <image id="ibl" offset="0,0" />
  <image id="ibr" offset="64,0" />
 </imageatlas>
 <tileset>
  <tile id="bl"><image ref="ibl" /></tile>
  <tile id="v"><image ref="iv" /></tile>
  <tile id="tl"><image ref="itl" /></tile>
  <tile id="h"><image ref="ih" /></tile>
  <tile id="e"><image ref="ie" /></tile>
  <tile id="br"><image ref="ibr" /></tile>
  <tile id="tr"><image ref="itr" /></tile>
 </tileset>
<rectmap id="test" tile_size="32x32">
 <column>
  <cell tile="bl" />
  <cell tile="v" />
  <cell tile="tl" />
 </column>
 <column>
  <cell tile="h" />
  <cell tile="e" />
  <cell tile="h" />
 </column>
 <column>
  <cell tile="br" />
  <cell tile="v" />
  <cell tile="tr" />
 </column>
</rectmap>
</resource>
